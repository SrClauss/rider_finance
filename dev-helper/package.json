{
  "name": "dev-helper",
  "displayName": "Dev Helper",
  "description": "Extensão para abrir terminais e iniciar serviços de desenvolvimento (backend, frontend, nginx) de forma automatizada.",
  "version": "0.2.0",
  "engines": {
    "vscode": ">=1.70.0"
  },
  "categories": ["Other"],
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:dev-helper.startDevServices"
  ],
  "contributes": {
    "commands": [
      {
        "command": "dev-helper.startDevServices",
        "title": "Dev Helper: Start All Dev Services"
      },
      {
        "command": "dev-helper.killAllTerminals",
        "title": "Dev Helper: Kill All Terminals"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "typescript": "5.8.2"
      }
}
